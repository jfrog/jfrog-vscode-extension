{
    "name": "jfrog-vscode-extension",
    "displayName": "JFrog",
    "description": "JFrog Xray scanning of npm project dependencies.",
    "version": "1.0.1",
    "license": "Apache-2.0",
    "icon": "resources/extensionIcon.png",
    "repository": {
        "type": "git",
        "url": "https://github.com/jfrog/jfrog-vscode-extension"
    },
    "bugs": {
        "url": "https://github.com/jfrog/jfrog-vscode-extension/issues"
    },
    "publisher": "JFrog",
    "engines": {
        "vscode": "^1.37.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "*"
    ],
    "main": "./out/extension.js",
    "contributes": {
        "viewsContainers": {
            "activitybar": [
                {
                    "id": "package-explorer",
                    "title": "Package Explorer",
                    "icon": "resources/extensionIcon.png"
                }
            ]
        },
        "views": {
            "package-explorer": [
                {
                    "id": "jfrog.xray",
                    "name": "Components Tree"
                },
                {
                    "id": "jfrog.xray.component",
                    "name": "Component Details"
                },
                {
                    "id": "jfrog.xray.issues",
                    "name": "Component Issues Details"
                }
            ]
        },
        "commands": [
            {
                "command": "jfrog.xray.refresh",
                "title": "Refresh",
                "icon": {
                    "light": "resources/light/refresh.png",
                    "dark": "resources/dark/refresh.png"
                }
            },
            {
                "command": "jfrog.xray.connect",
                "title": "Connect",
                "icon": "resources/connect.png"
            },
            {
                "command": "jfrog.xray.filter",
                "title": "Filter",
                "icon": {
                    "light": "resources/light/filter.png",
                    "dark": "resources/dark/filter.png"
                }
            },
            {
                "command": "jfrog.xray.showInProjectDesc",
                "title": "Show in project descriptor"
            }
        ],
        "menus": {
            "view/title": [
                {
                    "command": "jfrog.xray.refresh",
                    "when": "view == jfrog.xray",
                    "group": "navigation"
                },
                {
                    "command": "jfrog.xray.connect",
                    "when": "view == jfrog.xray",
                    "group": "navigation"
                },
                {
                    "command": "jfrog.xray.filter",
                    "when": "view == jfrog.xray",
                    "group": "navigation"
                }
            ],
            "view/item/context": [
                {
                    "command": "jfrog.xray.showInProjectDesc",
                    "when": "view == jfrog.xray"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "format": "prettier --write \"src/**/*.ts\" \"package.json\" \"tsconfig.json\" \"tslint.json\"",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "lint": "tslint -p tsconfig.json",
        "pretest": "npm run compile",
        "test": "node ./out/test/runTest.js",
        "package": "vsce package"
    },
    "dependencies": {
        "xray-client-js": "^1.0.0",
        "typescript-collections": "1.3.2",
        "keytar": "4.11.0",
        "semver": "6.3.0"
    },
    "devDependencies": {
        "@types/chai": "^4.2.0",
        "@types/faker": "4.1.2",
        "@types/fs-extra": "^8.0.0",
        "@types/glob": "^7.1.1",
        "@types/keytar": "^4.4.0",
        "@types/mocha": "^5.2.7",
        "@types/semver": "^6.0.1",
        "@types/vscode": "^1.37.0",
        "chai": "4.2.0",
        "faker": "^4.1.0",
        "fs-extra": "^8.1.0",
        "glob": "^7.1.4",
        "mocha": "^6.2.0",
        "prettier": "^1.16.4",
        "tslint": "^5.12.1",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^3.5.3",
        "vsce": "^1.66.0",
        "vscode-test": "^1.2.0"
    },
    "keywords": [
        "vulnerabilities",
        "artifactory",
        "javascript",
        "typescript",
        "security",
        "nodejs",
        "devops",
        "jfrog",
        "xray",
        "scan",
        "npm",
        "js",
        "ts"
    ]
}
